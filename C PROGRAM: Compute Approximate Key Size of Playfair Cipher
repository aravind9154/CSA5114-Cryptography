#include <stdio.h>
#include <math.h>

// Function to compute log2(25!)
double log2_factorial(int n) {
    double sum = 0.0;
    for (int i = 1; i <= n; i++) {
        sum += log2(i);
    }
    return sum;
}

int main() {
    int n = 25;

    double log2_value = log2_factorial(n);

    printf("Number of possible Playfair keys = 25!\n");
    printf("Approximate value = 2^%.4f\n", log2_value);

    return 0;
}
